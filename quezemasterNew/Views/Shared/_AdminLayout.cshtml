/*     .fixed-bottomss {
        position: fixed;
        right: 0;
        bottom: 0;
        left: 0;
        background: #DBE0FF;
        color: #000;
    } */


    .fixed-bottomss {
        background: #DBE0FF;
        color: #000;
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
      
    }

    html, body {
        height: 100%;
    }

    body {
        display: flex;
        flex-direction: column;
    }

    main {
        flex: 1; /* pushes footer to bottom */
    }


    @@media(min - width: 348px)

    and (max-width: 770px) {
        .col-12 p

    {
        line-height: 0.8;
    }

    }

    .fixed-bottomss .row {
        margin: 0;
    }


    /* Responsive Design */
    @@media (max-width: 767.98px) {
        .fixed-bottomss .col-md-2, .fixed-bottomss .col-md-6, .fixed-bottomss .col-md-3

    {
        text-align: center;
        margin-top: 150px;
    }

    .fixed-bottomss .col-md-2.text-end,
    .fixed-bottomss .col-md-6.text-end,
    .fixed-bottomss .col-md-3.text-end {
        text-align: center;
    }

    .casenameh1 {
        font-size: 1px;
        /* font-weight: bold; */
        margin-left: 16px;
        margin-bottom: 0;
        color: #000;
    }

    }

    @@media (min-width: 768px) {
        .fixed-bottomss .col-md-2.text-end, .fixed-bottomss .col-md-6.text-end, .fixed-bottomss .col-md-3.text-end

    {
        text-align: end;
    }

    }
</style>
