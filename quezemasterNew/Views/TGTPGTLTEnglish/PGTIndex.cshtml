@model quezemasterNew.Models.TGTPGTLT.TGTPGTLTViewModel
@{
    ViewData["Title"] = "PGT";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container-fluid">

    <div id="PGTEnglishDetailsForm">
        @await Component.InvokeAsync("PGTEnglish",
        new { ViewComponentType = "PGTEnglishForm", TGTPGTLTEnglishDetails = Model })
    </div>


    <div class="row justify-content-center mt-3">
        <div class="col-md-12 card card-body shadow-sm CardCss" id="PGTDetailsList">
            @await Component.InvokeAsync("PGTEnglish", new { ViewComponentType = "PGTEnglishList", TGTPGTLTEnglishDetails = Model })
        </div>
    </div>

</div>
