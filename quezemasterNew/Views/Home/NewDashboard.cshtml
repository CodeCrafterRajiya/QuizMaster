<script>

var $ = jQuery.noConflict();
$("#SubmitbuttonID").click(function () {
    var Name = $("#name").val();
    var Email = $("#email").val();
    var Mobile = $("#mob").val();
    var Message = $("#Messagecomment").val();

    var data = {
        Name: Name,
        Email: Email,
        Mobile: Mobile,
        Message: Message
    }
    
	$.ajax({
		type: "POST",
		url: "/Home/EnquiryFormNew",
		data: { data: JSON.stringify(data) },
        success: function (response) {
            
			location.reload(true);

			var Name = $("#name").html('');
    var Email = $("#email").html('');
    var Mobile = $("#mob").html('');
    var Message = $("#Messagecomment").html('');

        }
    }); 
}); 

		

</script>